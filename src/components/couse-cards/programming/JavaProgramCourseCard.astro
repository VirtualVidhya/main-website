---
import CourseCardLayout from "../../../layouts/CourseCardLayout.astro";
import CourseCardToolTag from "../../CourseCardToolTag.astro";

const courseCardDetails = {
  refLink: "/courses/programming/java-programming/",
  icon: "/images/course-icons/java-programming.svg",
  lazyLoadIconImg: false,
  name: "Java Programming",
  description:
    "Build robust and scalable applications with Java programming...",
  duration: "2-5",
};
---

<CourseCardLayout {courseCardDetails}>
  <slot slot="course-tool-tags-slot">
    <CourseCardToolTag toolName="Java" />
  </slot>
</CourseCardLayout>
