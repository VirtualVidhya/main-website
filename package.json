{
  "name": "vvidhya.com",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "functions/api/submit-contact-form.js",
  "scripts": {
    "build:contact-form": "esbuild functions/api/submit-contact-form.js --bundle --platform=node --external:resend --outfile=dist/worker.js",
    "build:umami-proxy": "esbuild functions/api/umami-proxy.js --bundle --platform=node --external:resend --outfile=dist/umami-worker.js",
    "build:worker": "npm run build:contact-form && npm run build:umami-proxy",
    "build:vite": "vite build",
    "build": "npm run build:worker && npm run build:vite",
    "build:vite-watch": "vite build --watch",
    "dev": "npm run build:worker && wrangler dev",
    "dev:vite": "vite",
    "deploy": "wrangler deploy",
    "purgecss": "purgecss --css src/styles/fontawesome.css --content './**/*.html' --output src/styles/fontawesome.min.css",
    "log": "npx wrangler pages deployment tail"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.2",
    "autoprefixer": "^10.4.20",
    "fast-glob": "^3.3.2",
    "postcss": "^8.4.49",
    "purgecss": "^6.0.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.5",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.8",
    "wrangler": "^3.92.0"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-web": "^0.40.1",
    "esbuild": "^0.24.0",
    "lottie-web": "^5.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.1",
    "swiper": "^11.1.15"
  }
}
